FUNCTION _0yu10wzwx     && Read Level Show
PARAMETERS lcShowWin
lcShowWin=IIF(PARAM()>0 AND TYPE('lcShowWin')='C',lcShowWin,'')
PRIVATE currwind
STORE WOUTPUT() TO currwind
*
* Show Code from screen: ALGAPLG
*
#REGION 1
IF ((SYS(2016) = UPPER(gcbasewind) OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER(gcbasewind) $ UPPER(lcShowWin)         
  _0YU10WZX5=IIF(WVISIBLE((gcbasewind)),'SAME','NOSHOW')
	ACTIVATE WINDOW (gcbasewind) &_0YU10WZX5
ENDIF
*
* Show Code from screen: ALGAPLG1
*
#REGION 3
IF ((SYS(2016) = UPPER(lcwinkey) OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER(lcwinkey) $ UPPER(lcShowWin)         
  _0YU10WZXC=IIF(WVISIBLE((lcwinkey)),'SAME','NOSHOW')
	ACTIVATE WINDOW (lcwinkey) &_0YU10WZXC

  @ 2.308,65.200 SAY IIF(!EMPTY(laData[4]),IIF(SEEK("M"+laData[4],'Customer'),Customer.btName,""),"") ;
	SIZE 1.000,54.800, 0.000 ;
		FONT "MS Sans Serif", 9 ;
		   COLOR (gcSayColor)

  @ 4.154,13.600 SAY lcCusPo ;
	SIZE 1.000,37.800, 0.000 ;
		FONT "MS Sans Serif", 9 ;
		   COLOR (gcSayColor)
ENDIF
*
