  *E300579,1 Hesham El-Sheltawi (Start)
  *E300579,1 Check if the Activation Key contain that
  *E300579,1 the runing version is a Demo Version
  ENDFOR
  IF llValdKey = .T.
    EXIT
  ENDIF
  *E300579,1 Hesham El-Sheltawi (End)
ENDFOR
  IF !llValdKey
    WAIT "Invalid Activation Key" WINDOW
