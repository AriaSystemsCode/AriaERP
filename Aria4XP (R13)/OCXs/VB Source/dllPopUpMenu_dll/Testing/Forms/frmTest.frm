Attribute VB_Name = "frmTest"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Dim WithEvents mobjMenu As AriaPMenu.cPopupMenu
Attribute mobjMenu.VB_VarHelpID = -1

Private Sub Form_Load()
  Call InitializeRegion
  
  Set mobjMenu = New AriaPMenu.cPopupMenu
  mobjMenu.hWndOwner = hWnd
  mobjMenu.ImageList = imlMain
  
  mobjMenu.AddItem "Yasser Mohammed Aly", , , , 31 - 1, , , "YMA"
  mobjMenu.AddItem "Mohammed AbdelSalam", , , , 14 - 1, , , "MAN"
  mobjMenu.AddItem "Sherein Hemdan Moh.", , , , 56 - 1, , , "SHM"
End Sub

Private Sub InitializeRegion()
  Set picEmp.Picture = Nothing
  lblName.Caption = "Employee Name"
End Sub

Private Sub Form_MouseDown(Button As Integer, Shift As Integer, X As Single, Y As Single)
  If Button = vbRightButton Then
    Call mobjMenu.ShowPopupMenu(X, Y)
    Call InitializeRegion
  End If
End Sub

Private Sub mobjMenu_Click(ItemNumber As Long)
  MsgBox mobjMenu.Caption(ItemNumber), vbInformation
End Sub

Private Sub mobjMenu_ItemHighlight(ItemNumber As Long, bEnabled As Boolean, bSeparator As Boolean)
  If bEnabled And Not bSeparator Then
    lblName.Caption = mobjMenu.Caption(ItemNumber)
    
    Dim lngIndex As Long
    lngIndex = mobjMenu.ItemIcon(ItemNumber)
    If lngIndex > -1 Then
      Set picEmp.Picture = imlMain.ListImages(lngIndex + 1).Picture
    Else
      Set picEmp.Picture = Nothing
    End If
  End If
End Sub
