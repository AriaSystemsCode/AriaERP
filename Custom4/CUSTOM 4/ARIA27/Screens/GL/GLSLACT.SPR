		SIZE 1,1, 0
	
	  @ 1,5 SAY "" ;
		SIZE 1,1, 0
	
	  @ 3,5 SAY "" ;
		SIZE 1,1, 0
	
	lcWindNoGr = ''
	
	IF NOT WVISIBLE("awdglslact")
		ACTIVATE WINDOW awdglslact     
	ENDIF     
	
	READ CYCLE MODAL ;
		WHEN _1up12v7rw()
	
	  RELEASE WINDOW awdglslact
	RELEASE POPUPS _1up12v7q7
	
	#REGION 0
	IF m.talkstat = "ON"
