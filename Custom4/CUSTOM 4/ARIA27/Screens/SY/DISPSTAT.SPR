PROCEDURE LFVPRINT
DO CASE
CASE _DOS
	      IF PSETUP(.T.)
	        SET DEVICE TO PRINT
	        SET CONSOLE OFF
	        LIST STATUS TO PRINT
	        SET CONSOLE ON
	        DO GFENDPRN
	     ENDIF
	RETURN
	
CASE _WINDOWS
	      IF PSETUP(.T.)
	        SET DEVICE TO PRINT
	        SET CONSOLE OFF
	        LIST STATUS TO PRINT
	        SET CONSOLE ON
	        lcOGPlatForm='DOS'
	        DO GFENDPRN
	     ENDIF
	RETURN
	
ENDCASE
