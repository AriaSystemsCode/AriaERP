FUNCTION _1up1421hi     && Read Level Show
PARAMETERS lcShowWin
lcShowWin=IIF(PARAM()>0 AND TYPE('lcShowWin')='C',lcShowWin,'')
PRIVATE currwind
STORE WOUTPUT() TO currwind
*
* Show Code from screen: PSCLSRG
*
#REGION 1
IF ((SYS(2016) = UPPER(gcbasewind) OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER(gcbasewind) $ UPPER(lcShowWin)         
  _1UP1421HP=IIF(WVISIBLE((gcbasewind)),'SAME','NOSHOW')
	ACTIVATE WINDOW (gcbasewind) &_1UP1421HP
ENDIF
*
* Show Code from screen: PSCLSRG3
*
#REGION 4
IF ((SYS(2016) = UPPER(lcwinch3) OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER(lcwinch3) $ UPPER(lcShowWin)         
  _1UP1421HQ=IIF(WVISIBLE((lcwinch3)),'SAME','NOSHOW')
	ACTIVATE WINDOW (lcwinch3) &_1UP1421HQ

  @ 3.077,53.800 SAY lnCatNetSl+lnOpenBaln-(lnTotAdjst) ;
	SIZE 1.000,11.500, 0.000 ;
		FONT "MS Sans Serif", 8 ;
		 STYLE "B"

  @ 1.077,38.800 SAY ldsydate ;
	SIZE 1.000,11.800, 0.000 ;
		FONT "MS Sans Serif", 8

  @ 5.000,26.200 SAY lnOpenBaln ;
	SIZE 1.000,14.000, 0.000 ;
		FONT "MS Sans Serif", 8

  @ 5.000,75.600 SAY lnTotSales ;
	SIZE 1.000,14.000, 0.000 ;
		FONT "MS Sans Serif", 8

  @ 6.538,26.200 SAY -(lnTotAdjst) ;
	SIZE 1.000,14.000, 0.000 ;
		FONT "MS Sans Serif", 8

  @ 6.538,75.600 SAY lnTotRetrn ;
	SIZE 1.000,14.000, 0.000 ;
		FONT "MS Sans Serif", 8

  @ 8.077,75.600 SAY lnNetClose ;
	SIZE 1.000,14.000, 0.000 ;
		FONT "MS Sans Serif", 8
ENDIF
IF NOT EMPTY(currwind)
  _1UP1421I4=IIF(WVISIBLE(currwind),'SAME','NOSHOW')
	ACTIVATE WINDOW (currwind) &_1UP1421I4   
ENDIF