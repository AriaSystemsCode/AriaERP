*       * Called By:           WHEN Clause                        
*       * Snippet Number:      17                                 
*       *                                                         
*       *********************************************************
*
FUNCTION _1up13j6cn     &&  ibTrap04 WHEN
#REGION 9
lnLastKey = LASTKEY()
lcWLast   = UPPER(WLAST())

DO CASE
  CASE lnLastKey # 15
    KEYBOARD "{ALT+Q}" PLAIN CLEAR

  CASE lnLastKey = 15 AND lcWLast = UPPER(lcCTChWn42)
    KEYBOARD "{ALT+Q}" PLAIN CLEAR

  CASE lnLastKey = 15 AND lcWLast = UPPER(lcLinesBrw)
      ACTIVATE WINDOW (lcFolder)
      _CUROBJ = OBJNUM(ibFolder[2])

  CASE lnLastKey = 15 AND lcWLast = UPPER("gwcContrl1")
    DO CASE
      CASE llCTZoom
        KEYBOARD "{ALT+Q}" PLAIN CLEAR

      CASE !llCTZoom AND lcActTrnBr $ "RO"
        KEYBOARD "{ALT+J}" PLAIN CLEAR

      CASE !llCTZoom AND lcActTrnBr = "B"
        IF laScrMode[2]
          KEYBOARD "{ALT+Q}" PLAIN CLEAR
        ELSE
          _CUROBJ = OBJNUM(pbRem)
        ENDIF

      CASE !llCTZoom AND lcActTrnBr = "T"
        KEYBOARD "{ALT+Q}" PLAIN CLEAR
    ENDCASE
ENDCASE


