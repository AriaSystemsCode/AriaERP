FUNCTION _1up13iqbo     && Read Level Show
PARAMETERS lcShowWin
lcShowWin=IIF(PARAM()>0 AND TYPE('lcShowWin')='C',lcShowWin,'')
PRIVATE currwind
STORE WOUTPUT() TO currwind
*
* Show Code from screen: MFISLOG
*
#REGION 1
IF ((SYS(2016) = UPPER("mfislog") OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER("mfislog") $ UPPER(lcShowWin)         
  _1UP13IQDO=IIF(WVISIBLE("mfislog"),'SAME','NOSHOW')
	ACTIVATE WINDOW mfislog &_1UP13IQDO
ENDIF
*
* Show Code from screen: MFISLOG0
*
#REGION 2
IF ((SYS(2016) = UPPER("mfislog0") OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER("mfislog0") $ UPPER(lcShowWin)         
  _1UP13IQDP=IIF(WVISIBLE("mfislog0"),'SAME','NOSHOW')
	ACTIVATE WINDOW mfislog0 &_1UP13IQDP

IF m.cCatgTyp $ 'FT'
    @ 2.667,1.500 SAY SUBSTR(m.Item,1,7) ;
	  SIZE 1.000,17.000, 0.000 ;
		  FONT "MS Sans Serif", 9
ENDIF 

IF m.cCatgTyp $ 'FT'
    @ 2.667,14.625 SAY m.IClr ;
	  SIZE 1.000,14.000, 0.000 ;
		  FONT "MS Sans Serif", 9
ENDIF 

IF llItemDye
    @ 2.667,25.875 SAY m.Dyelot ;
	  SIZE 1.000,25.000, 0.000 ;
		  FONT "MS Sans Serif", 9
ENDIF 

IF m.cCatgTyp $ 'FS' .OR. (m.cCatgTyp='T' .AND. m.Trim_Invt)
    @ 2.667,67.750 SAY lnOnhand ;
	  SIZE 1.000,14.000, 0.000 ;
		  FONT "MS Sans Serif", 9
ENDIF 

  @ 2.667,45.250 SAY m.Req_Qty ;
	SIZE 1.000,14.000, 0.000 ;
		FONT "MS Sans Serif", 9

  @ 2.667,56.500 SAY m.Used_Qty ;
	SIZE 1.000,14.000, 0.000 ;
		FONT "MS Sans Serif", 9

IF m.cCatgTyp $ 'FTS'
    @ 4.750,1.500 SAY m.Desc ;
	  SIZE 1.000,64.000, 0.000 ;
		  FONT "MS Sans Serif", 9
ENDIF 

IF m.cCatgTyp='S'
    @ 0.500,1.500 SAY lcItmHdr ;
	  SIZE 1.000,LEN(lcItmHdr)+.5, 0.000 ;
		  FONT "Courier", 10
ENDIF 

IF m.cCatgTyp='S'
    @ 2.667,1.500 SAY m.Item ;
	  SIZE 1.000,LEN(lcItmMsk)+.5, 0.000 ;
		  FONT "Courier", 10 ;
		  PICTURE "@! " + lcItmMsk
ENDIF 

IF m.cCatgTyp = 'M'
    @ 2.667,1.500 SAY m.Desc ;
	  SIZE 1.000,64.000, 0.000 ;
		  FONT "MS Sans Serif", 9
ENDIF 
ENDIF
IF NOT EMPTY(currwind)
  _1UP13IQE9=IIF(WVISIBLE(currwind),'SAME','NOSHOW')
	ACTIVATE WINDOW (currwind) &_1UP13IQE9   
ENDIF