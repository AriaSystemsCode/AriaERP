*
#REGION 1
IF ((SYS(2016) = UPPER(gcbasewind) OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER(gcbasewind) $ UPPER(lcShowWin)         
  _1UP138V06=IIF(WVISIBLE((gcbasewind)),'SAME','NOSHOW')
	ACTIVATE WINDOW (gcbasewind) &_1UP138V06
ENDIF
*
* Show Code from screen: MAPRCAM1
*
#REGION 3
IF ((SYS(2016) = UPPER(lcwinch1) OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER(lcwinch1) $ UPPER(lcShowWin)         
  _1UP138V0R=IIF(WVISIBLE((lcwinch1)),'SAME','NOSHOW')
	ACTIVATE WINDOW (lcwinch1) &_1UP138V0R

  @ 0.769,3.833 SAY lcPrgtype ;
	SIZE 1.000,10.333, 0.000 ;
		FONT "MS Sans Serif", 8 ;
		 STYLE "B"

  @ 4.000,16.500 SAY lcVenName ;
	SIZE 1.000,48.000, 0.000 ;
		FONT "MS Sans Serif", 9

  @ 4.000,60.000 SAY lcWareDEsc ;
	SIZE 1.000,48.000, 0.000 ;
