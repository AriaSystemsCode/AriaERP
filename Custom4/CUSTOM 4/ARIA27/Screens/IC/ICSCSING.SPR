	SIZE 1.000,7.667, 0.000 ;
		FONT "MS Sans Serif", 8 ;
		 STYLE "B" ;
		PICTURE "@TJ XXXXXXXX" ;
		  COLOR RGB(,,,192,192,192)

  @ 2.154,61.400 SAY "Size5" ;
	SIZE 1.000,7.833, 0.000 ;
		FONT "MS Sans Serif", 8 ;
		 STYLE "B" ;
		PICTURE "@TJ XXXXXXXX" ;
		  COLOR RGB(,,,192,192,192)

  @ 2.154,73.200 SAY "Size6" ;
	SIZE 1.000,7.667, 0.000 ;
		FONT "MS Sans Serif", 8 ;
		 STYLE "B" ;
		PICTURE "@TJ XXXXXXXX" ;
		  COLOR RGB(,,,192,192,192)

  @ 2.154,85.000 SAY "Size7" ;
	SIZE 1.000,7.667, 0.000 ;
		FONT "MS Sans Serif", 8 ;
		 STYLE "B" ;
		PICTURE "@TJ XXXXXXXX" ;
		  COLOR RGB(,,,192,192,192)

  @ 2.154,96.800 SAY "Size8" ;
	SIZE 1.000,7.667, 0.000 ;
		FONT "MS Sans Serif", 8 ;
		 STYLE "B" ;
		PICTURE "@TJ XXXXXXXX" ;
		  COLOR RGB(,,,192,192,192)
ENDIF
IF NOT EMPTY(currwind)
  _1UP1337FY=IIF(WVISIBLE(currwind),'SAME','NOSHOW')
	ACTIVATE WINDOW (currwind) &_1UP1337FY   
ENDIF