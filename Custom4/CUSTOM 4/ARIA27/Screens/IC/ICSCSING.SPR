#REGION 6
IF ((SYS(2016) = UPPER(lcwinc12) OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER(lcwinc12) $ UPPER(lcShowWin)         
  _1UP1337FQ=IIF(WVISIBLE((lcwinc12)),'SAME','NOSHOW')
	ACTIVATE WINDOW (lcwinc12) &_1UP1337FQ

  @ 2.154,14.000 SAY "Size1" ;
	SIZE 1.000,7.667, 0.000 ;
		FONT "MS Sans Serif", 8 ;
		 STYLE "B" ;
		PICTURE "@TJ XXXXXXXX" ;
