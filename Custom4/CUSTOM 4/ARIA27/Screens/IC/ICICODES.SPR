FUNCTION _1up132ga0     && Read Level Show
PARAMETERS lcShowWin
lcShowWin=IIF(PARAM()>0 AND TYPE('lcShowWin')='C',lcShowWin,'')
PRIVATE currwind
STORE WOUTPUT() TO currwind
*
* Show Code from screen: ICICODES
*
#REGION 1
IF ((SYS(2016) = UPPER(gcbasewind) OR SYS(2016) = "*") AND EMPTY(lcShowWin)) OR UPPER(gcbasewind) $ UPPER(lcShowWin)         
  _1UP132GAF=IIF(WVISIBLE((gcbasewind)),'SAME','NOSHOW')
	ACTIVATE WINDOW (gcbasewind) &_1UP132GAF

  @ 20.417,2.750 SAY lcOldMask ;
	SIZE 1.000,18.500, 0.000 ;
		FONT "FoxFont", 9

  @ 20.417,23.750 SAY lnOldTotal ;
	SIZE 1.000,2.125, 0.000 ;
		FONT "FoxFont", 9 ;
		PICTURE "99"

  @ 20.500,37.875 SAY lcMask ;
	SIZE 1.000,19.500, 0.000 ;
		FONT "FoxFont", 9 ;
		  COLOR RGB(0,0,0,,,,)

  @ 20.417,59.875 SAY lnTotal ;
	SIZE 1.000,2.125, 0.000 ;
		FONT "FoxFont", 9 ;
		PICTURE "99" ;
		  COLOR RGB(0,0,0,,,,)

  @ 20.417,29.750 SAY lnOldNoSeg ;
	SIZE 1.000,2.125, 0.000 ;
		FONT "FoxFont", 9 ;
		PICTURE "99"

  @ 20.417,65.625 SAY lnNoSeg ;
	SIZE 1.000,2.125, 0.000 ;
		FONT "FoxFont", 9 ;
		PICTURE "99"

  @ 15.250,47.000 SAY lcItemHead ;
	SIZE 1.000,35.200, 0.000 ;
		FONT "MS Sans Serif", 9
ENDIF
IF NOT EMPTY(currwind)
  _1UP132GAY=IIF(WVISIBLE(currwind),'SAME','NOSHOW')
	ACTIVATE WINDOW (currwind) &_1UP132GAY   
ENDIF