Public Event Quiting()
Public Function AddEntry(ByVal intCode As Integer, ByVal strCommand As String, ByVal strResponse As String)
  With grdLogEntries
    .AddItem strCommand
    .Cell(flexcpText, .Rows - 1, 0) = intCode
    .Cell(flexcpText, .Rows - 1, 1) = strCommand
    .Cell(flexcpText, .Rows - 1, 2) = strResponse
    .TopRow = .Rows - 1
    .Refresh
  End With
End Function
Private Sub Form_QueryUnload(Cancel As Integer, UnloadMode As Integer)
  RaiseEvent Quiting
  Hide
  Cancel = True
End Sub
Private Sub Form_Resize()
  With grdLogEntries
    .Move 15, 40, ScaleWidth - 30, IIf(ScaleHeight - 60 < 0, 0, ScaleHeight - 60)
    .ColWidth(0) = 500
    .ColWidth(1) = 1000
    .ColWidth(2) = .Width - (.ColWidth(0) + .ColWidth(1)) - 350
  End With
End Sub
